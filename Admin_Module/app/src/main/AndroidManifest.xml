        <activity
            android:name=".Submission.Submission_activity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Notice.DeleteNotice"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".teachers.UpdateFaculty"
            android:parentActivityName=".teachers.UploadFaculty" />
