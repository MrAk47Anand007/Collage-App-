        <activity
            android:name=".teachers.UploadFaculty"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".UploadOldPaper"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".UploadCarriculum"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".UploadBook"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".UploadImage"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Notice.upload_notice"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">

        </activity>
    </application>

</manifest>