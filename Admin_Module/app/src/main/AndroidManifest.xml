        android:theme="@style/Theme.GPA">
        <activity android:name=".submissionView.MarksActivity">

        </activity>
        <activity android:name=".auth.auth">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".submissionView.pdfViewactivity"
            android:parentActivityName=".submissionView.subViewPDf" />
        <activity
            android:name=".submissionView.subViewPDf"
            android:parentActivityName=".submissionView.Submission_View" />
        <activity
            android:name=".submissionView.Submission_View"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Submission.delete_submission"
